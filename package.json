{"name": "matrix-theme","displayName": "Matrix Theme","description": "Enter the Matrix with animated rain effects, customizable green terminals, and an immersive cyberpunk coding experience","version": "0.0.1","publisher": "your-publisher","engines": {"vscode": "^1.85.0"},"categories": ["Themes", "Other"],"keywords": ["matrix", "theme", "animation", "cyberpunk", "green", "terminal", "effects", "rain"],"main": "./out/extension.js","activationEvents": ["onStartupFinished"],"contributes": {"commands": [{"command": "matrix-theme.toggleRain","title": "Matrix: Toggle Rain Effect"},{"command": "matrix-theme.enterMatrix","title": "Matrix: Enter the Matrix"},{"command": "matrix-theme.exitMatrix","title": "Matrix: Exit the Matrix"},{"command": "matrix-theme.showMatrixTerminal","title": "Matrix: Open Matrix Terminal"},{"command": "matrix-theme.toggleGlitch","title": "Matrix: Toggle Glitch Effect"}],"configuration": {"title": "Matrix Theme","properties": {"matrixTheme.rainEffect.enabled": {"type": "boolean","default": true,"description": "Enable the matrix rain effect"},"matrixTheme.rainEffect.speed": {"type": "number","default": 50,"minimum": 10,"maximum": 200,"description": "Speed of the matrix rain effect (ms)"},"matrixTheme.rainEffect.density": {"type": "number","default": 0.8,"minimum": 0.1,"maximum": 2.0,"description": "Density of the matrix rain characters"},"matrixTheme.rainEffect.characters": {"type": "string","default": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789@#$%^&*()_+-=[]{}|;:,.<>?","description": "Characters to use in the matrix rain"},"matrixTheme.terminal.greenMode": {"type": "boolean","default": true,"description": "Use green matrix terminal colors"},"matrixTheme.glitch.enabled": {"type": "boolean","default": false,"description": "Enable glitch effects"},"matrixTheme.glitch.intensity": {"type": "number","default": 0.3,"minimum": 0.1,"maximum": 1.0,"description": "Intensity of glitch effects"}}},"menus": {"commandPalette": [{"command": "ma